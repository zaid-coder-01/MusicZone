<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>servelet.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>log</servlet-name>
        <servlet-class>servelet.log</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newuser</servlet-name>
        <servlet-class>servelet.newuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savesong</servlet-name>
        <servlet-class>servelet.savesong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savelist</servlet-name>
        <servlet-class>servelet.savelist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savelistsong</servlet-name>
        <servlet-class>servelet.savelistsong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveart</servlet-name>
        <servlet-class>servelet.saveart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delsong</servlet-name>
        <servlet-class>servelet.delsong</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dellist</servlet-name>
        <servlet-class>servelet.dellist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saverecent</servlet-name>
        <servlet-class>servelet.saverecent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servelet.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>log</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newuser</servlet-name>
        <url-pattern>/newuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savesong</servlet-name>
        <url-pattern>/savesong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savelist</servlet-name>
        <url-pattern>/savelist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savelistsong</servlet-name>
        <url-pattern>/savelistsong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveart</servlet-name>
        <url-pattern>/saveart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delsong</servlet-name>
        <url-pattern>/delsong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dellist</servlet-name>
        <url-pattern>/dellist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saverecent</servlet-name>
        <url-pattern>/saverecent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
        <servlet>
            <servlet-name>NewServlet</servlet-name>
            <servlet-class>servelet.NewServlet</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>NewServlet</servlet-name>
            <url-pattern>/NewServlet</url-pattern>
        </servlet-mapping>
    </session-config>
</web-app>
